{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "projects": {
    "AdminDashboard": {
      "projectType": "application",
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/AdminDashboard",
            "index": "src/index.html",
            "browser": "src/main.ts",
            "polyfills": ["src/polyfills.ts"],
            "tsConfig": "tsconfig.app.json",
            "assets": 
            [
          
            {
              "glob": "**/*",
              "input": "src/styles/images",
              "output": "/assets/"
            },
            {
              "glob": "**/*",
              "input": "public",
              "output": "/assets/"
            },
            {
              "glob": "favicon.ico", 
              "input": "src/",
              "output": "/"
            }
          ],
            "styles": ["src/styles.scss"],
            "stylePreprocessorOptions": {
              "includePaths": ["src/styles"]
            },
            "inlineStyleLanguage": "scss",
            "sourceMap": true
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2MB",
                  "maximumError": "5MB"
                }
              ],
              "outputHashing": "all",
              "sourceMap": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "buildTarget": "AdminDashboard:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "AdminDashboard:build:production"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "tsconfig.spec.json"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": ["src/**/*.ts"]
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "62161d38-0190-489e-8a05-276d508f3659"
  }
}
